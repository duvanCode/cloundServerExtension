{
    "manifest_version": 3,
    "name": "Server Clound",
    "description": "Server Clound",
    "version": "1.0",
    "action": {
        "default_popup": "login.html",
        "default_icon": "images/hello_extensions.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "downloads"
    ],
    "host_permissions": [
        "http://localhost:3001/*",
        "http://localhost:3000/*",
        "https://cloundserverappback.onrender.com/*",
        "https://cloundserver.onrender.com/*",
        "http://telegram.ondeploy.space/*",
        "http://serverclound.lego.ondeploy.space/*",
        "https://cdn.jsdelivr.net/*",
        "https://telegram.ondeploy.space/*"
    ],
      "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}