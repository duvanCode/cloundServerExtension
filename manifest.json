{
    "manifest_version": 3,
    "name": "Server Clound",
    "description": "Server Clound",
    "version": "1.0",
    "action": {
        "default_popup": "login.html",
        "default_icon": "images/hello_extensions.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "downloads"
    ],
    "host_permissions": [
        "http://localhost:3001/*"
    ]
}